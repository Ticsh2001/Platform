{
    "name": "NeuralModel",
    "author": "Developer 1",
    "version": "0.1",
    "description": "Simple neural model train object",
    "category": "Machine Learning",
    "class_path": null,
    "in_ports":
    [
        {
            "name": "train",
            "use_port":"data_machine_learning_port"
        },
        {
            "name": "val",
            "use_port":"data_machine_learning_port"
        },
        {
            "name": "loss_function",
            "use_port": "loss_function_machine_learning_port"
        },
        {
            "name": "optimizer",
            "use_port":"optimizer_machine_learning_port"            
        },
        {
            "name": "callbacks",
            "use_port": "callback_function_machine_learning_port"
        },
        {
            "name": "metrics",
            "description": "Метрики качества при обучении",
            "values": 
            [
                {
                    "param_name": "MetricsObjects",
                    "value_class": "General.general"
                }
            ]
        }
    ],
    "out_ports": 
    [
        {
            "name": "model",
            "description":"Обученная нейронная сеть",
            "values": 
            [
                {
                    "param_name": "neural_model",
                    "value_class": "General.general"
                }
            ]
        }
    ],
    "parameters": 
    [
        {
            "name": "architecture",
            "value_class": "General.general",
            "value": ""
        },
        {
            "name": "model_options",
            "value_class": "General.general",
            "value": ""
        },
        {
            "name": "epochs",
            "value_class": "General.general"
        },
        {
            "name": "history",
            "value_class": "General.general"
        },
        {
            "name": "refit",
            "value_class": "General.general"
        }
    ],
    "functions": 
    {
        "calculate_func": "MachineLearning.models.tf_neural_model.calculate",
        "update_int_conn_func": null,
        "setup_func": "MachineLearning.models.tf_neural_model.setup_element"
    }
}